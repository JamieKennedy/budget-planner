{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    },
    "ConnectionStrings": {
        "Default": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=BudgetPlanner;Integrated Security=True;Connect Timeout=30;Encrypt=False;"
    },
    "CorsPolicy": {
        "Name": "Localhost",
        "AllowedOrigins": [
            "http://localhost:5173"
        ],
        "AllowedMethods": "*",
        "AllowedHeaders":  "*",
        "AlowCredentials":  true
    }
}

